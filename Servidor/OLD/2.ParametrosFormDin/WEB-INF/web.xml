<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>MontarFormulario</servlet-name>
		<servlet-class>Generar</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MontarFormulario</servlet-name>
		<url-pattern>/MontarFormulario</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ProcesarFormulario</servlet-name>
		<servlet-class>Respuesta</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ProcesarFormulario</servlet-name>
		<url-pattern>/ProcesarFormulario</url-pattern>
	</servlet-mapping>

</web-app>
