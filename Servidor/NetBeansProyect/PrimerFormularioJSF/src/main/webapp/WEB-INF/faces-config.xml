<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/mostrardatos.jsp</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/index.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.jsp</from-view-id>
        <navigation-case>
            <from-outcome>mostrar</from-outcome>
            <to-view-id>/mostrardatos.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <description>datos de formulario de index.jsp</description>
        <managed-bean-name>datos</managed-bean-name>
        <managed-bean-class>com.daw.primerformulariojsf.datos</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
